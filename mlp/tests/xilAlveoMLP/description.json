{
    "gui": false,
    "name": "Xilinx HPC FCN Kernel Test", 
    "description": "Xilinx HPC FCN Test", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u50"
    ], 
    "launch": [
        {
            "name": "generic launch for all flows", 
            "cmd_args": "BUILD/mlp.xclbin"
        }
    ], 
    "host": {
        "host_exe": "xilAlveoMLP.so", 
        "compiler": {
            "sources": [
                "LIB_DIR/mlp/src/sw/pythonapi.cpp", 
                "LIB_DIR/L2/common/src/sw/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/mlp/externals/", 
                "LIB_DIR/mlp/include/", 
                "LIB_DIR/mlp/include/sw/", 
                "LIB_DIR/L1/hpc/include/",
                "LIB_DIR/L2/common/include/sw", 
                "LIB_DIR/utils/include/sw" 
            ], 
            "symbols": [
                "HW_CLK=3.33e-9",
                "HPC_instrBytes=64",
                "HPC_dataType=float", 
                "HPC_numChannels=16",
                "HPC_vecChannels=2",
                "HPC_maxInstrs=100",
                "HPC_parEntries=8"
            ], 
            "options": "$(shell python -m pybind11 --includes) -I$(MKLROOT)/include"
        },
        "linker": {
            "options": "-fopenmp -shared -fPIC"
        }
    }
}
