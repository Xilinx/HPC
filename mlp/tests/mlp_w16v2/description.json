{
    "gui": false,
    "name": "Xilinx FCN Kernel", 
    "description": "Xilinx HPC FCN Test", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u50"
    ], 
    "platform_blacklist": [
        "vck"
    ], 
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "cflags": [
                    ]
                }
            },
            "containers": [
                {
                    "name": "mlp", 
                    "ldclflags": "--config PROJECT/conn_u50.cfg"
                }
            ]
        }
    }, 
    "pre_launch": [
        {
            "launch_cmd": "make data_gen"
        }
    ], 
    "launch": [
        {
            "name": "generic launch for all flows", 
            "cmd_args": "BUILD/mlp.xclbin"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "linker": {
            "options": "-fopenmp"
        },
        "compiler": {
            "sources": [
                "LIB_DIR/mlp/src/sw/c++/mlpTest.cpp", 
                "LIB_DIR/L2/common/src/sw/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/mlp/externals/", 
                "LIB_DIR/mlp/include/", 
                "LIB_DIR/mlp/include/sw/", 
                "LIB_DIR/L1/hpc/include/",
                "LIB_DIR/L2/common/include/sw", 
                "LIB_DIR/utils/include/sw" 
            ], 
            "symbols": [
                "HW_CLK=3.33e-9",
                "HPC_instrBytes=64",
                "HPC_dataType=float", 
                "HPC_numChannels=16",
                "HPC_vecChannels=2",
                "HPC_maxInstrs=100",
                "HPC_parEntries=8"
            ]
        }
    }, 
    "v++": {
        "compiler": {
            "symbols": [
                "HPC_instrBytes=64",
                "HPC_dataType=float", 
                "HPC_maxInstrs=100",
                "HPC_numActFuncs=1",
                "HPC_maxVecSize=4096",
                "HPC_bufferSize=16384",
                "HPC_numChannels=16",
                "HPC_vecChannels=2",
                "HPC_parEntries=8"
            ], 
            "includepaths": [
                "LIB_DIR/mlp/include/", 
                "LIB_DIR/mlp/include/hw/", 
                "LIB_DIR/L1/hpc/include/hw/mlp/", 
                "LIB_DIR/L1/blas/include/", 
                "LIB_DIR/L1/blas/include/hw/", 
                "LIB_DIR/L1/blas/include/hw/xf_blas", 
                "LIB_DIR/L1/hpc/include/",
                "LIB_DIR/L1/hpc/include/hw",
                "LIB_DIR/L2/common/include/hw"
            ]
        }
    }, 
    "containers": [
        {
            "name": "mlp", 
            "ldclflags": "--config PROJECT/opts.cfg", 
            "frequency": 300, 
            "accelerators": [
                {
                    "name": "krnl_fcn", 
                    "location": "LIB_DIR/mlp/src/hw/fcnKernel.cpp", 
                    "frequency": 300
                }
            ]
        }
    ], 
    "testinfo": {
        "disable": true, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 4096, 
                    "vitis_hw_emu": 1024, 
                    "vitis_hw_run": 1024
                }, 
                "max_time_min": 240
            }
        ], 
        "targets": [
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
