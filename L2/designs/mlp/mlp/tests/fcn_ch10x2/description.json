{
    "gui": false,
    "name": "Xilinx FCN Kernel", 
    "description": "Xilinx HPC FCN Test", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u50"
    ], 
    "platform_blacklist": [
        "vck"
    ], 
    "platform_properties": {
        "u50": {
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config hls.cfg"
                    ]
                }
            },
            "containers": [
                {
                    "name": "mlp", 
                    "ldclflags": "--config PROJECT/conn_u50.cfg"
                }
            ]
        }
    }, 
    "pre_launch": [
        {
            "launch_cmd": "make data_gen"
        }
    ], 
    "launch": [
        {
            "name": "generic launch for all flows", 
            "cmd_args": "BUILD/mlp.xclbin"
        }
    ], 
    "host": {
        "host_exe": "host.exe", 
        "compiler": {
            "sources": [
                "LIB_DIR/L2/mlp/src/sw/fcnTest.cpp", 
                "LIB_DIR/../Vitis_Libraries/blas/L2/src/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "LIB_DIR/L2/mlp/include/", 
                "LIB_DIR/L2/mlp/include/sw/", 
                "LIB_DIR/../Vitis_Libraries/blas/../hpc/L2/include/", 
                "LIB_DIR/../Vitis_Libraries/blas/../hpc/L2/include/common", 
                "LIB_DIR/../Vitis_Libraries/blas/L1/include/", 
                "LIB_DIR/../Vitis_Libraries/blas/L2/include/memKernel/sw/", 
                "LIB_DIR/../Vitis_Libraries/blas/L1/tests/sw/include/", 
                "LIB_DIR/../Vitis_Libraries/blas/L2/include/xcl2"
            ], 
            "symbols": [
                "HW_CLK=3e-9",
                "HPC_instrBytes=64",
                "HPC_dataType=float", 
                "HPC_numChannels=10",
                "HPC_vecChannels=2",
                "HPC_maxInstrs=50",
                "HPC_parEntries=8"
            ], 
            "options": "-O3"
        }
    }, 
    "v++": {
        "compiler": {
            "symbols": [
                "HPC_instrBytes=64",
                "HPC_dataType=float", 
                "HPC_maxInstrs=50",
                "HPC_numActFuncs=1",
                "HPC_maxVecSize=4096",
                "HPC_numChannels=10",
                "HPC_vecChannels=2",
                "HPC_parEntries=8"
            ], 
            "includepaths": [
                "LIB_DIR/L2/mlp/include/", 
                "LIB_DIR/L2/mlp/include/hw/", 
                "LIB_DIR/../Vitis_Libraries/blas/../hpc/L2/include/common", 
                "LIB_DIR/../Vitis_Libraries/blas/L1/include/", 
                "LIB_DIR/../Vitis_Libraries/blas/L1/include/hw/", 
                "LIB_DIR/../Vitis_Libraries/blas/L1/include/hw/xf_blas", 
                "LIB_DIR/../Vitis_Libraries/hpc/L1/include/hw",
                "LIB_DIR/../Vitis_Libraries/hpc/L2/include/hw"
            ]
        }
    }, 
    "containers": [
        {
            "name": "mlp", 
            "ldclflags": "--config PROJECT/opts.cfg", 
            "frequency": 333, 
            "accelerators": [
                {
                    "name": "krnl_fcn", 
                    "location": "LIB_DIR/L2/mlp/src/hw/fcnKernel.cpp", 
                    "frequency": 333
                }
            ]
        }
    ], 
    "testinfo": {
        "disable": true, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 4096, 
                    "vitis_hw_emu": 1024, 
                    "vitis_hw_run": 1024
                }, 
                "max_time_min": 240
            }
        ], 
        "targets": [
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
